zeta <- function(z, g){
g2 <- g[1L]
g3 <- g[2L]
e <- polyroot(c(-g3, -g2, 0, 4))
e1 <- e[1L]
e2 <- e[2L]
e3 <- e[3L]
p <- wp(z, g)
2*Carlson_RG(p-e1, p-e2, p-e3, minerror = 1e-6) - z*p
}
